<UIElement class="UIElement">
    <id>game_over_root</id>
    <controller ui="GAME_OVER"/>
    <children>
        <UIButton class="UIButton">
            <id>game_over_play_button</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>160</y>
                </coord>
            </position>
            <onClick>play</onClick>
            <sprite>
                <sprite>button_normal</sprite>
                <scale>4</scale>
            </sprite>
            <hover>
                <sprite>button_hover</sprite>
                <scale>4</scale>
            </hover>
            <clicked>
                <sprite>button_click</sprite>
                <scale>4</scale>
            </clicked>
            <locked>
                <sprite>button_locked</sprite>
                <scale>4</scale>
            </locked>
            <hitShape>hitshape_button</hitShape>
            <children>
                <UILabel class="UILabel">
                    <id>game_over_play_button_label</id>
                    <position>
                        <coord>
                            <x>640</x>
                            <y>160</y>
                        </coord>
                    </position>
                    <text>Play Again</text>
                    <scale>2</scale>
                </UILabel>
            </children>
        </UIButton>
        <UIButton class="UIButton">
            <id>game_over_main_button</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>80</y>
                </coord>
            </position>
            <onClick>main</onClick>
            <sprite>
                <sprite>button_normal</sprite>
                <scale>4</scale>
            </sprite>
            <hover>
                <sprite>button_hover</sprite>
                <scale>4</scale>
            </hover>
            <clicked>
                <sprite>button_click</sprite>
                <scale>4</scale>
            </clicked>
            <locked>
                <sprite>button_locked</sprite>
                <scale>4</scale>
            </locked>
            <hitShape>hitshape_button</hitShape>
            <children>
                <UILabel class="UILabel">
                    <id>game_over_main_button_label</id>
                    <position>
                        <coord>
                            <x>640</x>
                            <y>80</y>
                        </coord>
                    </position>
                    <text>Main menu</text>
                    <scale>2</scale>
                </UILabel>
            </children>
        </UIButton>
        <UILabel class="UILabel">
            <id>game_over_text</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>480</y>
                </coord>
            </position>
            <text>GAME OVER</text>
            <scale>12</scale>
        </UILabel>
    </children>
</UIElement>