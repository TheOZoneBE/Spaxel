<UIElement class="UIElement">
    <id>class_selection_root</id>
    <controller ui="CLASS_SELECTION"/>
    <children>
        <UIButton class="UIButton">
            <id>class_select_white_select</id>
            <position>
                <coord>
                    <x>190</x>
                    <y>360</y>
                </coord>
            </position>
            <onClick>selectWhite</onClick>
            <sprite>
                <sprite>white</sprite>
                <scale>4</scale>
            </sprite>
            <hover>
                <sprite>class_select_hover</sprite>
                <scale>4</scale>
            </hover>
            <clicked>
                <sprite>class_select_click</sprite>
                <scale>4</scale>
            </clicked>
            <hitShape>hitshape_class_select</hitShape>
            <children>
                <UIVisual class="UIVisual">
                    <id>class_select_white_sprite</id>
                    <position>
                        <coord>
                            <x>190</x>
                            <y>360</y>
                        </coord>
                    </position>
                    <sprite>
                        <sprite>white</sprite>
                        <scale>4</scale>
                    </sprite>
                </UIVisual>
                <Label class="UILabel">
                    <id>class_select_white_text</id>
                    <position>
                        <coord>
                            <x>190</x>
                            <y>300</y>
                        </coord>
                    </position>
                    <text>white\Standard ship</text>
                    <scale>1</scale>
                </Label>
            </children>
        </UIButton>
