<UIElement class="UIElement">
    <id>main_root</id>
    <controller ui="MAIN"/>
    <position>
        <coord>
            <x>0</x>
            <y>0</y>
        </coord>
    </position>
    <children>
        <UILabel class="UILabel">
            <id>main_title_label</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>600</y>
                </coord>
            </position>
            <text>SPAXEL</text>
            <scale>8</scale>
        </UILabel>
        <UIButton class="UIButton">
            <id>main_play_button</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>480</y>
                </coord>
            </position>
            <onClick>startGame</onClick>
            <sprite>
                <sprite>button_normal</sprite>
                <scale>4</scale>
            </sprite>
            <hover>
                <sprite>button_hover</sprite>
                <scale>4</scale>
            </hover>
            <clicked>
                <sprite>button_click</sprite>
                <scale>4</scale>
            </clicked>
            <locked>
                <sprite>button_locked</sprite>
                <scale>4</scale>
            </locked>
            <hitShape>hitshape_button</hitShape>
            <children>
                <UILabel class="UILabel">
                    <id>main_button_play_label</id>
                    <position>
                        <coord>
                            <x>640</x>
                            <y>480</y>
                        </coord>
                    </position>
                    <text>Play</text>
                    <scale>1.5</scale>
                </UILabel>
            </children>
        </UIButton>
        <UIButton class="UIButton">
            <id>main_achievements_button</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>400</y>
                </coord>
            </position>
            <onClick>achievements</onClick>
            <sprite>
                <sprite>button_normal</sprite>
                <scale>4</scale>
            </sprite>
            <hover>
                <sprite>button_hover</sprite>
                <scale>4</scale>
            </hover>
            <clicked>
                <sprite>button_click</sprite>
                <scale>4</scale>
            </clicked>
            <locked>
                <sprite>button_locked</sprite>
                <scale>4</scale>
            </locked>
            <disabled>true</disabled>
            <hitShape>hitshape_button</hitShape>
            <children>
                <UILabel class="UILabel">
                    <id>main_button_achievements_label</id>
                    <position>
                        <coord>
                            <x>640</x>
                            <y>400</y>
                        </coord>
                    </position>
                    <text>Achievements</text>
                    <scale>1.5</scale>
                </UILabel>
            </children>
        </UIButton>
        <UIButton class="UIButton">
            <id>main_options_button</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>320</y>
                </coord>
            </position>
            <onClick>options</onClick>
            <sprite>
                <sprite>button_normal</sprite>
                <scale>4</scale>
            </sprite>
            <hover>
                <sprite>button_hover</sprite>
                <scale>4</scale>
            </hover>
            <clicked>
                <sprite>button_click</sprite>
                <scale>4</scale>
            </clicked>
            <locked>
                <sprite>button_locked</sprite>
                <scale>4</scale>
            </locked>
            <hitShape>hitshape_button</hitShape>
            <children>
                <UILabel class="UILabel">
                    <id>main_button_options_label</id>
                    <position>
                        <coord>
                            <x>640</x>
                            <y>320</y>
                        </coord>
                    </position>
                    <text>Options</text>
                    <scale>1.5</scale>
                </UILabel>
            </children>
        </UIButton>
        <UIButton class="UIButton">
            <id>main_credits_button</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>240</y>
                </coord>
            </position>
            <onClick>credits</onClick>
            <sprite>
                <sprite>button_normal</sprite>
                <scale>4</scale>
            </sprite>
            <hover>
                <sprite>button_hover</sprite>
                <scale>4</scale>
            </hover>
            <clicked>
                <sprite>button_click</sprite>
                <scale>4</scale>
            </clicked>
            <locked>
                <sprite>button_locked</sprite>
                <scale>4</scale>
            </locked>
            <hitShape>hitshape_button</hitShape>
            <children>
                <UILabel class="UILabel">
                    <id>main_button_credits_label</id>
                    <position>
                        <coord>
                            <x>640</x>
                            <y>240</y>
                        </coord>
                    </position>
                    <text>Credits</text>
                    <scale>1.5</scale>
                </UILabel>
            </children>
        </UIButton>
        <UIButton class="UIButton">
            <id>main_quit_button</id>
            <position>
                <coord>
                    <x>640</x>
                    <y>80</y>
                </coord>
            </position>
            <onClick>quit</onClick>
            <sprite>
                <sprite>button_normal</sprite>
                <scale>4</scale>
            </sprite>
            <hover>
                <sprite>button_hover</sprite>
                <scale>4</scale>
            </hover>
            <clicked>
                <sprite>button_click</sprite>
                <scale>4</scale>
            </clicked>
            <locked>
                <sprite>button_locked</sprite>
                <scale>4</scale>
            </locked>
            <hitShape>hitshape_button</hitShape>
            <children>
                <UILabel class="UILabel">
                    <id>main_button_quit_label</id>
                    <position>
                        <coord>
                            <x>640</x>
                            <y>80</y>
                        </coord>
                    </position>
                    <text>Quit</text>
                    <scale>1.5</scale>
                </UILabel>
            </children>
        </UIButton>
    </children>
</UIElement>
